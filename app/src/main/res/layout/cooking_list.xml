            android:layout_width="0dp"
            android:layout_height="0dp"
            android:fontFamily="@font/vp_pixel_font"
            android:gravity="center"
            android:lines="1"
            android:text="9,999,999,999"
            android:textColor="@color/black"
            android:textStyle="bold"
            app:autoSizeMaxTextSize="100sp"
            app:autoSizeMinTextSize="20sp"
            app:autoSizeStepGranularity="5sp"
            app:autoSizeTextType="uniform"
            app:layout_constraintBottom_toTopOf="@+id/guideline24"
            app:layout_constraintEnd_toStartOf="@+id/line7"
            app:layout_constraintStart_toStartOf="@+id/line6"
            app:layout_constraintTop_toTopOf="@+id/guideline15" />

        <TextView
            android:id="@+id/alt_rsc_qtty_boosted"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:fontFamily="@font/vp_pixel_font"
            android:gravity="center"
            android:lines="1"
            android:text="9,999,999,999"
            android:textColor="@color/black"
            android:textStyle="bold"
            app:autoSizeMaxTextSize="100sp"
            app:autoSizeMinTextSize="20sp"
            app:autoSizeStepGranularity="5sp"
            app:autoSizeTextType="uniform"
            app:layout_constraintBottom_toTopOf="@+id/guideline14"
            app:layout_constraintEnd_toStartOf="@+id/line7"
            app:layout_constraintStart_toStartOf="@+id/line6"
            app:layout_constraintTop_toTopOf="@+id/guideline24" />


        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline9"
            android:layout_width="wrap_content"
